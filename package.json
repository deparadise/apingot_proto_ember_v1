{
	"name": "codePlanter_Ex_Em",
	"version": "0.0.1",
	"description": "Starting Back End API Project",
	"author": "David Pineda <david.pineda@deparadise.com>",
	"license": "MIT",
	"private": false,
	"homepage": "https://github.com/deparadise/codePlanter_Ex_Em#readme",
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/deparadise/codePlanter_Ex_Em.git"
	},
	"bugs": {
		"url": "https://github.com/deparadise/codePlanter_Ex_Em/issues"
	},
	"main": "index.js",
	"scripts": {
		"add_g_concurrently": "npm install -g concurrently",
		"clear_last_build": "rimraf client/ && rimraf dist/",

		"install_Ember": "npm install -g ember-cli",
		"check_Ember": "ember -v",
		"build_Ember": "cd FE_shovel && yarn && ember build",
		"start_Ember_dev": "cd FE_shovel && ember s --port 4500",

		"start_ingot": "nodemon index.js --exec babel-node --presets es2015,stage-2",
		"generate_dist": "babel ./ --out-dir dist/ --ignore ./node_modules,./FE_shovel,./client,./nodemon.json,./README.md,./yarn.lock,./.babelrc,./package.json,./npm-debug.log --copy-files",

		"Ember": "npm run install_Ember && npm run check_Ember && npm run build_Ember",
		"Ingot": "npm run generate_dist && node dist/index.js",

		"start": "npm run clear_last_build && npm run add_g_concurrently && concurrently \"npm run Ingot\" \"npm run Ember\""
	},
	"engines": {
		"node": "8.11.4",
		"npm": "6.4.0"
	},
	"dependencies": {
		"babel-cli": "^6.26.0",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-stage-2": "^6.24.1",
		"body-parser": "^1.18.3",
		"express": "^4.16.3",
		"passport": "^0.4.0",
		"passport-google-oauth20": "^1.0.0",
		"rimraf": "^2.6.2"
	},
	"devDependencies": {
		"eslint": "^4.19.1",
		"nodemon": "^1.17.3"
	}
}
